{
    "name": "graham-campbell/digitalocean",
    "description": "DigitalOcean Is A DigitalOcean Bridge For Laravel 5",
    "keywords": ["laravel", "framework", "digitalocean", "digitalocean-v2", "DigitalOceanV2", "digitalocean bridge", "bridge", "DigitalOcean", "Laravel DigitalOcean", "Laravel-DigitalOcean", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/contracts": "~5.0",
        "illuminate/support": "~5.0",
        "graham-campbell/manager": "~2.0",
        "toin0u/digitalocean-v2": "0.2.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0",
        "kriswallsmith/buzz": "~0.10",
        "guzzle/guzzle": "~3.7"
    },
    "suggest": {
        "kriswallsmith/buzz": "Enables the buzz connector.",
        "guzzle/guzzle": "Enables the guzzle connector."
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\DigitalOcean\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\DigitalOcean\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
